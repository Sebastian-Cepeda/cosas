<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BancoSoap">
                    <security mode="Transport" />
                </binding>
                <binding name="BancoSoap1" />
                <binding name="ImpuestosSoap">
                    <security mode="Transport" />
                </binding>
                <binding name="ImpuestosSoap1" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://localhost:44347/Banco.asmx" binding="basicHttpBinding"
                bindingConfiguration="BancoSoap" contract="Banco.BancoSoap"
                name="BancoSoap" />
            <endpoint address="https://localhost:44312/Impuestos.asmx" binding="basicHttpBinding"
                bindingConfiguration="ImpuestosSoap" contract="Impuestos.ImpuestosSoap"
                name="ImpuestosSoap" />
        </client>
    </system.serviceModel>
</configuration>